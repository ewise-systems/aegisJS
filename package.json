{
  "name": "ew-lib",
  "version": "0.0.1",
  "description": "Open source library for interacting with eWise Systems' Aegis 2.0 architecture",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:webpack",
    "build:tsc": "tsc --outDir build src/index.ts",
    "build:webpack": "npm run build:tsc && npx webpack --config webpack.config.js",
    "build:srv": "tsc srv/app.ts",
    "start": "node srv/app.js",
    "test": "mocha"
  },
  "keywords": [
    "ewise",
    "aegis"
  ],
  "author": "Rene Calunsag Jr, Jan Almazora",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-device": "^0.4.2",
    "express-useragent": "^1.0.12",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "whatwg-fetch": "^3.0.0"
  }
}
