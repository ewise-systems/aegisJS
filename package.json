{
  "name": "@ewise/aegisjs",
  "version": "0.1.1",
  "description": "Open source library for interacting with eWise Systems' Aegis 2.0 architecture",
  "main": "src/aegis.js",
  "scripts": {
    "prepublish": "npm run build",
    "clean": "rimraf dist",
    "lint": "jshint .",
    "bundle": "webpack --config webpack.config.js",
    "prebuild": "npm run clean",
    "build": "npm run lint && npm run bundle",
    "start": "npm run build && node srv/testApp.js",
    "preartifice": "rimraf .nyc_output coverage mocha_test_results.xml",
    "artifice": "npm run lint -- -- --reporter=xunit --reporter-options output=mocha_test_results.xml && nyc report"
  },
  "keywords": [
    "ewise",
    "aegis"
  ],
  "author": "Rene Calunsag Jr",
  "license": "MIT",
  "devDependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "covgen": "^3.1.0",
    "express": "^4.16.4",
    "fast-check": "^1.15.1",
    "jshint": "^2.10.2",
    "lodash": "^4.17.14",
    "mocha": "^6.1.4",
    "nyc": "^14.0.0",
    "source-map-support": "^0.5.12",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "@ewise/aegisjs-core": "^0.1.1",
    "ajv": "^6.10.0",
    "axios": "^0.18.1",
    "folktale": "^2.3.2",
    "ramda": "^0.26.1",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.1",
    "uniqid": "^5.0.3",
    "valid-url": "^1.0.9"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ewise-systems/aegisJS.git"
  },
  "files": [
    "lib/",
    "src/"
  ]
}
