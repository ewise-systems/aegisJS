{
  "name": "ew-lib",
  "version": "0.0.1",
  "description": "Open source library for interacting with eWise Systems' Aegis 2.0 architecture",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build",
    "prebuid": "npm run clean",
    "build": "npm run build:tsc && webpack --config webpack.config.js",
    "build:tsc": "tsc --target ES5 --outDir build src/*.ts lib/**/*.ts",
    "start": "tsc srv/app.ts && npm run build && node srv/app.js",
    "test": "mocha"
  },
  "keywords": [
    "ewise",
    "aegis"
  ],
  "author": "Rene Calunsag Jr, Jan Almazora",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-device": "^0.4.2",
    "express-useragent": "^1.0.12",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "@types/ramda": "^0.26.6",
    "data.maybe": "^1.2.3",
    "data.task": "^3.1.2",
    "data.validation": "^1.3.1",
    "ramda": "^0.26.1",
    "whatwg-fetch": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ewise-systems/ew-lib.git"
  }
}
